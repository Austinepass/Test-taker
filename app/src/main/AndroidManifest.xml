<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.android.gmattaker">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/test_icon"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/test_icon"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Numerical"></activity>
        <activity android:name=".NumericalTests"></activity>
        <activity android:name=".Verbal"></activity>
        <activity android:name=".VerbalTests"></activity>
        <activity android:name=".Logical"></activity>
        <activity android:name=".ResultActivity"
            android:parentActivityName=".NumericalTests">
        </activity>
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>